   "source": [
    "def is_palindrome(word):\n",
    "    word = word.lower().replace(\"\",\"\")\n",
    "    return word == word[::-1]\n",
    "text = \"mom\"\n",
    "if is_palindrome(text):\n",
    "    print(f\" '{text}' is a palindrome\")\n",
    "else:\n",
    "    print(f\"'{text}' is not a palindrome\")"
   ]
